{
  "name": "Terraform AWS Template",
  "dockerComposeFile": "./compose.yml",
  "service": "app",
  "workspaceFolder": "/home/user/app",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-azuretools.vscode-docker",
        "GitHub.copilot",
        "oderwat.indent-rainbow",
        "amazonwebservices.aws-toolkit-vscode",
